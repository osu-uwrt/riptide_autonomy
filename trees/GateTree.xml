            <input_port name="x1" required="false"/>
            <input_port name="x2" required="false"/>
            <input_port name="y1" required="false"/>
            <input_port name="y2" required="false"/>
            <input_port name="z1" required="false"/>
            <input_port name="z2" required="false"/>
        </Action>
        <Condition ID="CompareNums">
            <input_port name="a" required="false"/>
            <input_port name="b" required="false"/>
            <input_port name="test" required="false">can be &quot;&lt;&quot;, &quot;&gt;&quot;, or &quot;=&quot;.</input_port>
        </Condition>
        <Action ID="ComputeYawAngle">
            <output_port name="output" required="false"/>
            <input_port name="x" required="false"/>
            <input_port name="y" required="false"/>
        </Action>
        <SubTree ID="DisarmTorpedos"/>
        <SubTree ID="Dive">
            <input_port default="false" name="__shared_blackboard" required="false">If false (default), the Subtree has an isolated blackboard and needs port remapping</input_port>
            <input_port name="depth" required="false">depth to submerge to</input_port>
        </SubTree>
        <SubTree ID="DoSearch">
            <input_port default="false" name="__shared_blackboard" required="false">If false (default), the Subtree has an isolated blackboard and needs port remapping</input_port>
            <input_port name="frame" required="false">Prop to search for (ex. gman)</input_port>
            <input_port name="proximity" required="false">Proximity to prop estimate to search</input_port>
        </SubTree>
        <SubTree ID="DropMarker">
            <input_port default="false" name="__shared_blackboard" required="false">If false (default), the Subtree has an isolated blackboard and needs port remapping</input_port>
            <input_port name="dropper" required="false">marker dropper ID. 1 or 2</input_port>
        </SubTree>
        <Action ID="Error">
            <input_port name="message" required="false">The message to print.</input_port>
        </Action>
        <SubTree ID="FireTorpedo">
            <input_port default="false" name="__shared_blackboard" required="false">If false (default), the Subtree has an isolated blackboard and needs port remapping</input_port>
            <input_port name="torpedo_id" required="false"/>
        </SubTree>
        <SubTree ID="GateTask"/>
        <SubTree ID="GetActuatorStates">
            <input_port default="true" name="__shared_blackboard" required="false">If false (default), the Subtree has an isolated blackboard and needs port remapping</input_port>
        </SubTree>
        <Action ID="GetActuatorStatus">
            <output_port name="claw_state" required="false"/>
            <output_port name="dropper1_state" required="false"/>
            <output_port name="dropper2_state" required="false"/>
            <output_port name="torpedo1_state" required="false"/>
            <output_port name="torpedo2_state" required="false"/>
        </Action>
        <Action ID="GetOdometry">
            <output_port name="op" required="false"/>
            <output_port name="or" required="false"/>
            <output_port name="oy" required="false"/>
            <output_port name="x" required="false"/>
            <output_port name="y" required="false"/>
            <output_port name="z" required="false"/>
        </Action>
        <Action ID="GetSwitchState">
            <inout_port name="aux_switch_inserted" required="false"/>
            <inout_port name="kill_switch_inserted" required="false"/>
        </Action>
        <SubTree ID="GetTorpedoState">
            <input_port default="false" name="__shared_blackboard" required="false">If false (default), the Subtree has an isolated blackboard and needs port remapping</input_port>
            <input_port name="torpedo_id" required="false"/>
            <output_port name="torpedo_state" required="false"/>
        </SubTree>
        <Action ID="HeadingToPoint">
            <input_port name="currX" required="false"/>
            <input_port name="currY" required="false"/>
            <output_port name="heading" required="false"/>
            <input_port name="targX" required="false"/>
            <input_port name="targY" required="false"/>
        </Action>
        <Action ID="Info">
            <input_port name="message" required="false">The message to print.</input_port>
        </Action>
        <Condition ID="IsClawClosed">
            <input_port name="claw_state" required="false"/>
        </Condition>
        <Condition ID="IsClawError">
            <input_port name="claw_state" required="false"/>
        </Condition>
        <Condition ID="IsClawOpen">
            <input_port name="claw_state" required="false"/>
        </Condition>
        <Condition ID="IsClawUnknown">
            <input_port name="claw_state" required="false"/>
        </Condition>
        <Condition ID="IsDropperDropped">
            <input_port name="dropper_state" required="false"/>
        </Condition>
        <Condition ID="IsDropperError">
            <input_port name="dropper_state" required="false"/>
        </Condition>
        <Condition ID="IsDropperReady">
            <input_port name="dropper_state" required="false"/>
        </Condition>
        <Condition ID="IsTorpedoCharged">
            <input_port name="torpedo_state" required="false"/>
        </Condition>
        <Condition ID="IsTorpedoCharging">
            <input_port name="torpedo_state" required="false"/>
        </Condition>
        <Condition ID="IsTorpedoDisarmed">
            <input_port name="torpedo_state" required="false"/>
        </Condition>
        <Condition ID="IsTorpedoError">
