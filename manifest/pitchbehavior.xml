<?xml version="1.0" encoding="UTF-8"?>

<behavior name="PitchBehavior">

	<executable package_path="riptide_autonomy.pitchbehavior_sm" class="PitchBehaviorSM" />
	<tagstring></tagstring>
	<author>UWRT</author>
	<date>Sun Mar 01 2020</date>
	<description>
		Change pitch using parameters
	</description>


	<!-- Contained Behaviors -->

	<!-- Available Parameters -->
	<params>

		<param type="numeric" name="angle" default="0" label="angle" hint="Sets the angle">
			<min value="-90" />
			<max value="90" />
		</param>

		<param type="text" name="topic" default="/puddles/go_to_pitch" label="topic" hint="Sets the topic" />

	</params>


</behavior>