<?xml version="1.0" encoding="UTF-8"?>

<behavior name="YawBehavior">

	<executable package_path="riptide_autonomy.yawbehavior_sm" class="YawBehaviorSM" />
	<tagstring></tagstring>
	<author>UWRt</author>
	<date>Sun Mar 01 2020</date>
	<description>
		Changes the yaw using parameters
	</description>


	<!-- Contained Behaviors -->

	<!-- Available Parameters -->
	<params>

		<param type="text" name="topic" default="/puddles/go_to_yaw" label="topic" hint="Sets the topic" />

		<param type="numeric" name="angle" default="0" label="angle" hint="Sets the angle">
			<min value="-180" />
			<max value="180" />
		</param>

	</params>


</behavior>