<?xml version="1.0" encoding="UTF-8"?>

<behavior name="DepthParameter">

	<executable package_path="riptide_autonomy.depthparameter_sm" class="DepthParameterSM" />
	<tagstring></tagstring>
	<author>UWRT</author>
	<date>Sun Mar 01 2020</date>
	<description>
		Change depth using parameters
	</description>


	<!-- Contained Behaviors -->

	<!-- Available Parameters -->
	<params>

		<param type="numeric" name="depth" default="0" label="depth" hint="Sets the depth">
			<min value="-10" />
			<max value="10" />
		</param>

		<param type="text" name="topic" default="/puddles/go_to_depth" label="topic" hint="Sets the topic" />

	</params>


</behavior>