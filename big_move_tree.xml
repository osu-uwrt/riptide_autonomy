<?xml version="1.0"?>
<root main_tree_to_execute="BehaviorTree">
    <!-- ////////// -->
    <BehaviorTree ID="BehaviorTree">
        <Sequence>
            <Action ID="BigMoveState" orientation_w="1" orientation_x="0" orientation_y="0" orientation_z="0" v_pitch="0" v_roll="10" v_yaw="0" x="-4" y="0" z="-1"/>
            <Action ID="BigMoveState" orientation_w="1" orientation_x="0" orientation_y="0" orientation_z="0" v_pitch="" v_roll="" v_yaw="" x="-4" y="0" z="-1"/>
            <Action ID="BigMoveState" orientation_w="1" orientation_x="0" orientation_y="0" orientation_z="0" v_pitch="" v_roll="" v_yaw="" x="0" y="0" z="-1"/>
        </Sequence>
    </BehaviorTree>
    <!-- ////////// -->
    <TreeNodesModel>
        <Action ID="BigMoveState">
            <input_port default="1" name="orientation_w"/>
            <input_port default="0" name="orientation_x"/>
            <input_port default="0" name="orientation_y"/>
            <input_port default="0" name="orientation_z"/>
            <input_port name="v_pitch"/>
            <input_port name="v_roll"/>
            <input_port name="v_yaw"/>
            <input_port default="0" name="x"/>
            <input_port default="0" name="y"/>
            <input_port default="-1" name="z"/>
        </Action>
        <Action ID="FlattenCalculationState">
            <input_port default="-1" name="depth">Depth the robot sinks to</input_port>
            <output_port default="1" name="orientation_w"/>
            <output_port default="0" name="orientation_x"/>
            <output_port default="0" name="orientation_y"/>
            <output_port default="0" name="orientation_z"/>
            <output_port default="0" name="x"/>
            <output_port default="0" name="y"/>
            <output_port default="-1" name="z"/>
        </Action>
        <Action ID="toWorldFrameState">
            <input_port name="object"/>
            <input_port name="relative_orientation_w"/>
            <input_port name="relative_orientation_x"/>
            <input_port name="relative_orientation_y"/>
            <input_port name="relative_orientation_z"/>
            <input_port name="relative_x"/>
            <input_port name="relative_y"/>
            <input_port name="relative_z"/>
            <output_port name="world_orientation_w"/>
            <output_port name="world_orientation_x"/>
            <output_port name="world_orientation_y"/>
            <output_port name="world_orientation_z"/>
            <output_port name="world_x"/>
            <output_port name="world_y"/>
            <output_port name="world_z"/>
        </Action>
    </TreeNodesModel>
    <!-- ////////// -->
</root>

